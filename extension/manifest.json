{
    "manifest_version": 3,
    "name": "Copy-Cat",
    "version": "0.1.0",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": [
          "content.js"
        ]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "clipboardRead",
      "clipboardWrite"
    ],
  
    "description": "The ultimate copy/paste enhancer",
  
    "author": "riverv2000@gmail.com"
  }